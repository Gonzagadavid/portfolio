{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "jsx-a11y/mouse-events-have-key-events": "off",
        "max-len": "off",
        "react/jsx-filename-extension": "off",
        "react/no-unescaped-entities": "off",
        "import/no-extraneous-dependencies": "off",
        "no-undef": "off"
    }
}
